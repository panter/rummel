{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "dist/**",
        "build/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ]
    },
    "lint": {},
    "clean": {
      "cache": false
    }
  }
}